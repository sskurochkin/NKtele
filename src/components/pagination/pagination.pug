+include-style('pagination')
.paginations
	ul.paginations__list
		li.paginations__item.paginations__item--first
			a.paginations__link(href='#') Previous
		li.paginations__item.active
			a.paginations__link(href='#') 1
		li.paginations__item
			a.paginations__link(href='#') 22
		li.paginations__item
			a.paginations__link(href='#') 3
		li.paginations__item.paginations__item--last(disabled)
			a.paginations__link(href='#') Next
